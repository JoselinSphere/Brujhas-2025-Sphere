{
  "components/common/body": "<main class=\"body\" id=\"main-content\" role=\"main\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}}\r\n    {{/block}}\r\n\r\n    <div class=\"myheadeing\">\r\n        <h2 class=\"page-heading\">Compra por colecciones\r\n            <p>Una experiencia excepcional</p>\r\n            <span class=\"head-line\"></span>\r\n        </h2>\r\n    </div>\r\n\r\n    <div class=\"\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n\r\n    {{> components/common/modal/modal}}\r\n    {{> components/common/alert/alert-modal}}\r\n</main>\r\n<style>\r\n@font-face {\r\n  font-family: 'AvenirNextLTPro-regular';\r\n  src: url('{{cdn \"assets/custom/fonts/Avenir-next-lt-pro/AvenirNextLTPro-Regular.woff2\"}}') format('woff2'),\r\n       url('{{cdn \"assets/custom/fonts/Avenir-next-lt-pro/AvenirNextLTPro-Regular.woff\"}}') format('woff');\r\n  font-weight: normal;\r\n  font-style: normal;\r\n}\r\n@font-face {\r\n  font-family: 'AvenirNextLTPro-demi';\r\n  src: url('{{cdn \"assets/custom/fonts/Avenir-next-lt-pro/AvenirNextLTPro-Demi.woff2\"}}') format('woff2'),\r\n       url('{{cdn \"assets/custom/fonts/Avenir-next-lt-pro/AvenirNextLTPro-Demi.woff\"}}') format('woff');\r\n  font-weight: normal;\r\n  font-style: normal;\r\n}\r\n</style>",
  "components/common/modal/modal": "<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\r\n    {{> components/common/modal/modal-close-btn}}\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n",
  "components/common/alert/alert-modal": "<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\r\n    <div class=\"alert-icon error-icon\">\r\n        <span class=\"icon-content\">\r\n            <span class=\"line line-left\"></span>\r\n            <span class=\"line line-right\"></span>\r\n        </span>\r\n    </div>\r\n\r\n    <div class=\"alert-icon warning-icon\">\r\n        <div class=\"icon-content\">!</div>\r\n    </div>\r\n\r\n    <div class=\"modal-content\"></div>\r\n\r\n    <div class=\"button-container\">\r\n        <button type=\"button\" class=\"confirm button\" data-reveal-close>{{lang 'common.ok'}}</button>\r\n        <button type=\"button\" class=\"cancel button\" data-reveal-close>{{lang 'common.cancel'}}</button>\r\n    </div>\r\n</div>\r\n",
  "components/common/modal/modal-close-btn": "<button class=\"modal-close\"\r\n        type=\"button\"\r\n        title=\"{{lang 'common.close'}}\"\r\n        {{additional_attr}}\r\n>\r\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\r\n    <span aria-hidden=\"true\">&#215;</span>\r\n</button>\r\n"
}